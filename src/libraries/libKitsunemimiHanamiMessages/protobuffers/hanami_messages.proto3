syntax = "proto3";

enum ClusterProcessType {
  REQUEST_TYPE = 0;
  TRAIN_TYPE = 1;
}

enum ClusterDataType {
  INPUT_TYPE = 0;
  SHOULD_TYPE = 1;
  OUTPUT_TYPE = 2;
}

message ClusterIO_Message {
  string segmentName = 1;
  bool isLast = 2;
  ClusterProcessType processType = 3;
  ClusterDataType dataType = 4;
  uint64 numberOfValues = 5;
  repeated float values = 6 [packed=true];
}

enum UploadDataType 
{
    DATASET_TYPE = 0;
    CHECKPOINT_TYPE = 1;
}


message FileUpload_Message 
{
    string datasetUuid = 1;
    string fileUuid = 2;
    bool isLast = 3;
    UploadDataType type = 4;
    uint64 position = 5;
    bytes data = 6;
}

message FileUploadResponse_Message
{
    string datasetUuid = 1;
}

message ErrorLog_Message 
{
    string userId = 1;
    string component = 2;
    string errorMsg = 3;
    string context = 4;
    string values = 5;
}

message AuditLog_Message
{
    string userId = 1;
    string component = 2;
    string endpoint = 3;
    string type = 4;
}

message CheckpointPull_Message
{
    string location = 1;
}

message DatasetRequest_Message
{
    string location = 1;
    string columnName = 2;
}

message ResultPush_Message
{
    string uuid = 1;
    string name = 2;
    string userId = 3;
    string projectId = 4;
    string results = 5;
}
